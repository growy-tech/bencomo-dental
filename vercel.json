{   
    "headers": [
      {
        "source" : "/checkout/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" },
          { "key": "Expires", "value": "Sat, 26 Jul 1997 05:00:00 GMT" },
          { "key": "Pragma", "value": "no-cache" }
        ]
      }
    ],
    "version": 2,
    "builds": [
      { "src": "app.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "app.py" }
    ]
}